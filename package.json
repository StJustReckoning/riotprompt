{
  "name": "@riotprompt/riotprompt",
  "version": "0.0.13-dev.0",
  "keywords": [
    "prompt",
    "llm",
    "genai",
    "openai"
  ],
  "description": "Get it together, and organize your prompts.",
  "type": "module",
  "main": "./dist/riotprompt.cjs",
  "module": "./dist/riotprompt.js",
  "types": "./dist/riotprompt.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobrien/riotprompt"
  },
  "exports": {
    ".": {
      "types": "./dist/riotprompt.d.ts",
      "import": "./dist/riotprompt.js",
      "require": "./dist/riotprompt.cjs"
    }
  },
  "bin": {
    "riotprompt": "./dist/cli.cjs"
  },
  "scripts": {
    "build": "npm run lint && tsc --noEmit && vite build && vite build -c vite.config.cli.ts",
    "start": "dist/main.js",
    "dev": "vite",
    "watch": "vite build --watch",
    "test": "npm run test:coverage",
    "test:coverage": "vitest run --coverage",
    "test:debug": "vitest --run --coverage --reporter verbose",
    "test:readme": "doccident -c .markdown-doctest-setup.mjs README.md",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf dist",
    "precommit": "npm run lint && npm run test",
    "prepublishOnly": "npm run clean && npm run build",
    "docs:dev": "cd docs && cp ../README.md public/ && npm install && npm run dev",
    "docs:build": "cd docs && cp ../README.md public/ && npm install && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test",
    "docs:coverage": "cd docs && npm run coverage"
  },
  "author": "Tim O'Brien <tobrien@discursive.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@doccident/doccident": "^0.0.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@swc/core": "^1.12.11",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@theunwalked/cardigantime": "file:../cardigantime",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^5.3.3",
    "glob": "^11.0.3",
    "js-yaml": "^4.1.1",
    "marked": "^16.0.0",
    "openai": "^6.15.0",
    "tiktoken": "^1.0.22",
    "zod": "^4.0.2"
  }
}
